{
  "name": "JIRA Quick Finder",
  "version": "0.1",
  "manifest_version": 2,
  "description": "__MSG_appDesc__",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Open JIRA tickets fast",
    "default_popup": "popup.html"
  },
  "default_locale": "en",
  "options_page": "options.html",
  "permissions": [
    "storage"
  ],
  "author": "Tim 'xGhOsTkiLLeRx' Brust",
  "icons": {
      "128": "icon128.png", 
      "16": "icon16.png", 
      "48": "icon48.png"
  }
}
